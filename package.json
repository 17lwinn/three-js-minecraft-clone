{
	"name": "minecraft-clone",
	"version": "0.2.0",
	"description": "",
	"main": "src/server/index.js",
	"scripts": {
		"build": "NODE_ENV=production webpack -p --progress",
		"start": "NODE_ENV=production babel-node scripts/start",
		"dev": "NODE_ENV=development nodemon --exec babel-node -- scripts/start",
		"test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./tests/setup.js ./tests/test.js",
		"prod": "npm run build; npm start",
		"watch": "node src/server/builder.js",
		"ngrok": "ngrok http -subdomain=passwhiz 3000"
	},
	"author": "nickwanninger",
	"license": "ISC",
	"engines": {
		"node": "=7.7.2",
		"npm": ">=5.0.0"
	},
	"babel": {
		"presets": [
			"es2015",
			"stage-2",
			"react"
		],
		"plugins": [
			"transform-decorators-legacy",
			[
				"transform-runtime",
				{
					"polyfill": false,
					"regenerator": true
				}
			],
			"lodash",
			"syntax-async-functions",
			"transform-regenerator",
			"syntax-dynamic-import",
			"dynamic-import-webpack"
		]
	},
