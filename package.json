		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "6.6.0",
		"babel-preset-react": "6.5.0",
		"babel-preset-stage-2": "6.22.0",
		"body-parser": "1.17.2",
